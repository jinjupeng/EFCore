{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "PictureOptions": {
    "FileTypes": ".gif,.jpg,.jpeg,.png,.GIF,.JPG,.JPEG,.PNG,.BMP",
    "MaxSize": 10485760,
    "ImageBaseUrl": "D:\\dotnet\\imageServer"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;uid=sa;pwd=123456;Database=EfCore;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}
